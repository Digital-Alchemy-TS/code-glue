{
  "expo": {
    "name": "code-glue",
    "slug": "code-glue",
    "version": "1.0.0",
    "scheme": "codeglue",
    "userInterfaceStyle": "automatic",
    "platforms": ["web"],
    "plugins": ["expo-router", "expo-font"],
    "experiments": {
      "typedRoutes": true
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro",
      "output": "static"
    }
  }
}
